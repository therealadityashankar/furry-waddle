{
  "name": "furry-waddle",
  "version": "1.0.0",
  "description": "html forms + magic",
  "main": "src/furrywaddle.js",
  "scripts": {
    "build": "browserify src/furrywaddle.js -o dist/furrywaddle.js && rm -rf docs/* && jsdoc -r src -d docs/ && sass src/sass/furrywaddle.scss dist/css/furrywaddle.css",
    "test": "mocha",
    "clean": "rm -rf docs/ dist/",
    "package": "zip dist/furrywaddle.zip dist/css/* dist/furrywaddle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/therealadityashankar/furry-waddle.git"
  },
  "keywords": [
    "html",
    "forms",
    "magic"
  ],
  "author": "Aditya Shankar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/therealadityashankar/furry-waddle/issues"
  },
  "homepage": "https://github.com/therealadityashankar/furry-waddle#readme",
  "dependencies": {
    "html-pages": "github:therealadityashankar/html-pages"
  },
  "devDependencies": {
    "grunt": "*",
    "grunt-contrib-jshint": "*"
  }
}
